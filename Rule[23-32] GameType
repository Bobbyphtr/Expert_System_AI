RULE [23]
If [playersType] = "singleplayer"
and [genreType] = "survival"
Then [gameType] = "survival singleplayer"

RULE [24]
If [playersType] = "multiplayer"
and [genreType] = "survival"
Then [gameType] = "survival multiplayer"

